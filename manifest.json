{
	"name": "ForgeAI Chrome Extension",
	"version": "0.1",
	"manifest_version": 3,
	"description": "OpenAI copilot for web.",
	"background": {
		"service_worker": "service-worker.js"
	},
	"side_panel": {
		"default_path": "sidepanel.html"
	},
	"permissions": [
		"sidePanel",
		"contextMenus",
		"activeTab"
	]
}